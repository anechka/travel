        link(href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.3/css/font-awesome.min.css", rel="stylesheet")

        link(href="css/main.css", rel='stylesheet')

    body
        .container-fluid
            header
                .row
                    .col-md-12.gutter-0.banner-bg
                        nav.navbar.navbar-default.navigation
                            .container
                                .row
                                    .navbar-header.logo-container
                                        a.navbar-brand.logo-brand(href="index.html")
                                            img.logo(src="images/gohealthy-logo.png")

                                        button.navbar-toggle.collapsed.pull-right.btn-toggle(type="button", data-toggle="collapse", data-target="#navbarCollapse")
                                            span.sr-only Toggle navigation
                                            span Hotels
                                                i.fa.fa-bars.icon-bars

                                    .collapse.navbar-collapse.gutter-0(id="navbarCollapse")
                                        ul.nav.navbar-nav.main-navbar
                                            li
                                                a(href="#") Link1
                                            li
                                                a(href="") Link2
                                            li
                                                a(href="#") Link3
                                            li
                                                a(href="#") Link4

                                            li
                                                a(href="#") Link5

            section#main_content
                .row
                    .col-md-12.hotel-page-bg
                        .container.hotel-content-container
                            .col-md-12.padding-left-right-40.box-content
                                .row
                                    .col-md-12.margin-top-40
                                        iframe.CHECKFRONT_DROPLET(name='checkfront', src='http://gohealthy.checkfront.com/reserve/?inline=1&lang_id=en&provider=droplet&src=http%3A%2F%2Fgohealthy.travel&1471283407526', height='750', width='100%', frameborder='0', scrolling='yes', style='display: block; min-height: 60px; overflow: hidden;')

            .row
                footer
                    .container
                        .row

                            .col-md-3

                                ul.list-unstyled.list-inline.footer-nav
                                    li
                                        a(href="http://gohealthy.travel/reserve/") Bookings

                                    li
                                        a(href="http://gohealthy.travel/privacy.html", target="_blank") Privacy

                                    li
                                        a(href="http://gohealthy.travel/terms.html", target="_blank") Terms

                            .col-md-9 Go Healthy Travel.&nbsp
                                span.wellness Wellness Tourism&nbsp
                                | by Cape May Global Partners.


        script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js")
        script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js")
