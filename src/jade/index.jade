        script(type='text/javascript').
            function showStuff(id1, text, el2, el) {
                //show paragraph
                document.getElementById(id1).style.display = 'block';
                // show the text
                document.getElementById(text).style.visibility = 'visible';
                //show the link ...less
                document.getElementById(el2).style.display = 'inline-block';
                // hide the link ...more
                el.style.display = 'none';
            }

            function hideStuff(id1, text, el2, el) {

                //hide paragraph
                document.getElementById(id1).style.display = 'none';
                // hide the text
                document.getElementById(text).style.visibility = 'hidden';
                //show the link ...more
                document.getElementById(el2).style.display = 'inline-block';
                // hide the link ...less
                el.style.display = 'none';

            }

            function showContent(id1, id2, text, el2, el) {
                //show paragraph
                document.getElementById(id1).style.display = 'block';
                //show paragraph
                document.getElementById(id2).style.display = 'block';
                // show the text
                document.getElementById(text).style.visibility = 'visible';
                //show the link ...less
                document.getElementById(el2).style.display = 'inline-block';
                // hide the link ...more
                el.style.display = 'none';
            }

            function hideContent(id1, id2, text, el2, el) {

                //hide paragraph
                document.getElementById(id1).style.display = 'none';
                //hide paragraph
                document.getElementById(id2).style.display = 'none';
                // hide the text
                document.getElementById(text).style.visibility = 'hidden';
                //show the link ...more
                document.getElementById(el2).style.display = 'inline-block';
                // hide the link ...less
                el.style.display = 'none';

            }

